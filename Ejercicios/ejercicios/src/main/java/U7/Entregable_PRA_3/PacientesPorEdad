import java.util.Comparator;

public class pacientesPorEdad implements Comparator<Pacientes> {
    @Override
    public int compare(Pacientes p1, Pacientes p2) {
        if (p1.getEdad() > p2.getEdad()) {
            return 1;
        } else if (p1.getEdad() < p2.getEdad()) {
            return -1;
        } else {
            return 0;
        }
    }
}
